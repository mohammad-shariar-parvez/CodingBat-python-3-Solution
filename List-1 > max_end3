def max_end3(nums):
    new = []
    if nums[0] > nums[-1] :
        new.append(nums[0])
        return new*3

    elif nums[0] < nums[-1] :
        new.append(nums[-1])
        return new*3

print(max_end3([9,6,10]))

#######ALTERNATIVE SOLUTION WITH msx FUNCTION   #########

def max_end3(nums):

  return [nums[0]]*3

print(max_end3([10,8,6,10]))
