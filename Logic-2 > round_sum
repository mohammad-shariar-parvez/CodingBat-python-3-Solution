def round10 (n):
    if n >=10 and n % 10 >=5 :
        return 10+((n//10)*10)
    elif n >=10 and n % 10 <=5 :
        return ((n//10)*10)
    elif n <= 10 and n % 10 >=5 :
        return 10
    else:
        return 0

def round_sum(a,b,c):
    return round10(a)+ round10(b) + round10(c)


print(round_sum(4,4,6))
